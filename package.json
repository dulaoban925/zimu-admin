{
  "name": "zimu-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p icons:build utils:build && vue-tsc && vite build",
    "preview": "vite preview",
    "icons:build": "pnpm run -C packages/zimu-icons build",
    "utils:build": "pnpm run -C packages/zimu-utils build",
    "lint:check": "eslint --cache --ext .js,.ts,.jsx,tsx,vue src/",
    "lint:fix": "eslint --cache --ext .js,.ts,.jsx,tsx,vue,json . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@element-plus/icons": "^0.0.11",
    "@vueuse/core": "^10.1.2",
    "@zimu/utils": "workspace:^1.0.0",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "element-plus": "^2.3.5",
    "face-api.js": "^0.22.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^20.2.5",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "@zimu/icons": "workspace:^1.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.0",
    "vite": "^4.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run lint:check"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
